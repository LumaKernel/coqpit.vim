
Describe coqpit#annotate#
  Describe associate(xml, content)
    It associates text sent and XML got from CoqTop in sentence end
      Assert Equals(coqpit#annotate#associate('', ['bar.'], [0, 0]), [0, 0])
      Assert Equals(coqpit#annotate#associate('a b c .', ['a (* *)','bc .'], [0, 0]), [1, 4])
      Assert Equals(coqpit#annotate#associate('a b c .', ['a (**)','(* *) bc .', 'wow.'], [0, 0]), [1, 10])
      Assert Equals(coqpit#annotate#associate('abc.', ['a (*foo*)','(* *) bc . foo', '.'], [0, 0]), [1, 10])
      Assert Equals(coqpit#annotate#associate('abc "efg".', ['abc (**) "efg"', '','(* *) . hi.'], [0, 0]), [2, 7])
      Assert Equals(coqpit#annotate#associate('abc " e fg".', ['abc (**) " e fg"', '','(* *) .  (* *)'], [0, 0]), [2, 7])
      Assert Equals(coqpit#annotate#associate('abc " e (*fg".', ['abc (**) " e (*fg"', '','(* *) .', '', ''], [0, 0]), [2, 7])
    End
  End
End
